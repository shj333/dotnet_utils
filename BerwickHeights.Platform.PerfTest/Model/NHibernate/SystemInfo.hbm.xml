<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Virtify.Platform.PerfTest"
                   namespace="Virtify.Platform.PerfTest.Model">

  <class name="SystemInfo" table="PERF_SystemInfo">
    <id name="SystemInfoID" access="property">
      <generator class="identity" />
    </id>
    <property name="Component" access="property" />
    <property name="Name" access="property" />
    <property name="Value" access="property" />

    <many-to-one name="TestSuiteResult" column="TestSuiteResultID" class="TestSuiteResult" access="property" />
  </class>
</hibernate-mapping>
