<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Virtify.Platform.PerfTest"
                   namespace="Virtify.Platform.PerfTest.Model">

  <class name="TestSuiteResult" table="PERF_TestSuiteResult">
    <id name="TestSuiteResultID" access="property">
      <generator class="identity" />
    </id>
    <property name="UserID" access="field.camelcase" />
    <property name="StartTime" access="property" />
    <property name="EndTime" access="property" />
    <property name="ElapsedTimeMSecs" access="property" />
    <property name="Annotation" access="property" />

    <set name="SystemInfoList" cascade="all" lazy="false" access="property" inverse="true">
      <key column="TestSuiteResultID" />
      <one-to-many class="SystemInfo" />
    </set>
    <set name="TestResultList" cascade="all" lazy="false" access="property" inverse="true">
      <key column="TestSuiteResultID" />
      <one-to-many class="TestResult" />
    </set>
  </class>
</hibernate-mapping>
